{{#items}}
    <a class="list-group-item list-group-item-action flex-column align-items-start fadein" href="{{url}}">
        <div class="d-flex w-100 justify-content-between">
            <div class="images">
                <img src={{userimg}}>
                <i class="fa {{icon}}"></i>
            </div>
            <p>{{userfullname}} <small>{{text}}</small></p>
        </div>
    </a>
{{/items}}
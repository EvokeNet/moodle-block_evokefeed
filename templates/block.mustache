        {{#data}}
            <a href="{{{url}}}" class="list-group-item list-group-item-action">
                <i class="fa {{{icon}}}"></i> {{{text}}}
            </a>
        {{/data}}
    </ul>
</div>

{{#js}}
    require(['block_evokefeed/evokefeed'], function(EvokeFeed) {
        EvokeFeed.init();
    });
{{/js}}